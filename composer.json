{
    "name": "tumainimosha/laravel-coming-soon",
    "description": "Simple Comming Soon page",
    "type": "library",
    "license": "MIT",
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish"
        ],
        "post-update-cmd": [
            "php -r \"copy('src/config/comingsoon.php', 'config/comingsoon.php');\"",
            "php artisan vendor:publish"
        ]
    },
    "authors": [
        {
            "name": "Soft 'n' Web",
            "email": "info@softnweb.in"
        }
    ],
    "minimum-stability": "dev",
    "require": {},
    "autoload": {
        "psr-4": {
            "snw\\ComingSoon\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "snw\\ComingSoon\\ComingSoonServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15@dev"
    }
}
